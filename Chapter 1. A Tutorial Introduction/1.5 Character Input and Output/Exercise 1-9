#include <stdio.h>

int main(void)
{
    int c;
    int blank = 0;

    while ((c = getchar())!= EOF) {
        if (c == ' '){
            if (!blank){
                blank = 1;
                putchar(c);
            }
        } else {
            blank = 0;
            putchar(c);
        }

    }
    return 0;
}
